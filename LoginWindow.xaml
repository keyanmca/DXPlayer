<Window x:Class="DXPlayer.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="로그인" Height="149" Width="299" WindowStartupLocation="CenterOwner">
    <Canvas>
        <Label Content="ID" Canvas.Top="10" Canvas.Left="10"/>
        <Label Content="Password" Canvas.Top="40" Canvas.Left="10"/>
        <TextBox x:Name="txtBoxID" Height="23" Canvas.Left="85" Text="" Canvas.Top="10" Width="196" KeyDown="txtBoxID_KeyDown"/>
        <PasswordBox x:Name="passwd" Canvas.Left="85" Canvas.Top="46.16" Width="196" KeyDown="passwd_KeyDown"/>
        <Button x:Name="btnOK" Content="확인" Canvas.Left="156" Canvas.Top="87.04" Width="60" Click="btnOK_Click"/>
        <Button x:Name="btnCancel" Content="취소" Canvas.Left="221" Canvas.Top="87.04" Width="60" Click="btnCancel_Click"/>
    </Canvas>
</Window>
